// Update the prview image based on customer selected box and rose.
function UpdateImage(src,parm){
  $('#live_svg svg').fadeTo( "slow" , 0.5);
  //$("body").addClass("loader_process");
  $.ajax({
      type: "post",
      url: "https://customize.rosepops.com/index.php/customizerose/converimage",
      data:{src: src},
      success: function(returnval) {
        if(returnval != 'image_convert_error'){
            console.log('Image successfully convert base64 formate!!!');
            if(parm == 'cbox'){
              $("#live_svg svg image#cbox").attr("href",returnval);
              $('#live_svg svg').fadeTo( "slow" , 1);
            }
            if(parm == 'crose'){
              $("#live_svg svg image#crose").attr("href",returnval);
              $('#live_svg svg').fadeTo( "slow" , 1);
            }
          	$("#custom_image .feature-row__image_custom").hide();
            $("body").removeClass("loader_process");
        }else{
        	console.log('image convert found error return!!!');
          	$("body").removeClass("loader_process");
        }
      },
      error: function (returnval) {
          console.log('image convert found error!!!');
          $("body").removeClass("loader_process");
          console.log(returnval);
      }
   });
}
function AddImage(src,parm,x,y,w,h){
   $('#live_svg svg').fadeTo( "slow" , 0.5);
   //$("body").addClass("loader_process");
   $.ajax({
      type: "post",
      url: "https://customize.rosepops.com/index.php/customizerose/converimage",
      data:{src: src},
      success: function(returnval) {
        if(returnval != 'image_convert_error'){
            console.log('Image successfully convert base64 formate!!!');
            //console.log(returnval);
            var svgimg = document.createElementNS('http://www.w3.org/2000/svg','image');
            svgimg.setAttributeNS(null,'height',w);
            svgimg.setAttributeNS(null,'width',h);
            svgimg.setAttributeNS('http://www.w3.org/1999/xlink','href', returnval);
            svgimg.setAttributeNS(null,'x',x);
            svgimg.setAttributeNS(null,'y',y);
            svgimg.setAttributeNS(null, 'visibility', 'visible');
            svgimg.setAttributeNS(null, 'id', parm);
            $('#live_svg svg').append(svgimg);
            $('#live_svg svg').fadeTo( "slow" , 1);
            $("#custom_image .feature-row__image_custom").hide();
            $("body").removeClass("loader_process");
        }else{
        	console.log('image convert found error return!!!');
          	$("body").removeClass("loader_process");
        }
      },
      error: function (returnval) {
          console.log('image convert found error!!!');
          console.log(returnval);
          $("body").removeClass("loader_process");
      }
   });
}


function crossDomainAjax(e, o, t) {
    if ("XDomainRequest" in window && null !== window.XDomainRequest) {
        "", e = e + "?" + decodeURI(o);
        var i = new XDomainRequest;
        i.open("GET", e), i.onload = function() {
            t(i.responseText)
        }, i.onerror = function() {
            console.log("error"), _result = !1
        }, setTimeout(function() {
            i.send()

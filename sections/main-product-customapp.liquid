    $("body").addClass("videoUploadingloader");
    $("#video_message_hid").val('');
    $("#video_file_name_l").text('');
    var form_data = new FormData();
    var name = document.getElementById("uploade_video_c").files[0].name;
    if(typeof name != ''){
      var ext = name.split('.').pop().toLowerCase();
      if(jQuery.inArray(ext, ['mp4','mov','flv','avi','swf']) == -1){
        $('#uploade_video_c').val('');
        $('#video_message_hid').val('');
        $("#video_file_name_l").text('');
        $("body").removeClass("loader_process");
        $("body").removeClass("videoUploadingloader");
        alert("Sorry, " + name + " is invalid, allowed extensions are: mp4,mov,flv,avi,swf");
        return false;
      }
      //var oFReader = new FileReader();
      //oFReader.readAsDataURL(document.getElementById("uploade_video_c").files[0]);
      var f = document.getElementById("uploade_video_c").files[0];
      var fsize = f.size||f.fileSize;
    
      // Allow max 100MB
      var video_size = fsize / 1024 / 1024; ;
      //console.log(fsize);
      //console.log(video_size);
      if(video_size > 100){
       $('#uploade_video_c').val('');
       $('#video_message_hid').val('');
       $("#video_file_name_l").text('');
       $("body").removeClass("loader_process");
       $("body").removeClass("videoUploadingloader");
       alert("This file is too large to upload. The maximum supported file size: 100MB");
       return false;
      }else{
        //now upload the file using ajax
        form_data.append("uploade_video_c", document.getElementById('uploade_video_c').files[0]);
        $.ajax({
          url:"https://customize.rosepops.com/index.php/customizerose/uploadvideo",
          type:"POST",
          data:form_data,
          contentType: false,
          cache: false,
          processData: false,
          dataType: "json",
          success: function(data){
            if (data.flag == "true"){  
              if(data.video_path != ''){
                console.log("Video Name: "+data.video_path);
                var video_url = 'https://customize.rosepops.com/application/uploads/videofiles/'+data.video_path;
